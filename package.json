{
    "name": "novasheets",
    "version": "1.0.7",
    "description": "A simple, powerful, and versatile CSS preprocessor",
    "keywords": [
        "novasheets",
        "nvss",
        "css",
        "css preprocessor",
        "css nesting",
        "nested css",
        "preprocessor",
        "stylesheets",
        "parser"
    ],
    "files": [
        "bin/",
        "dist/",
        "vars/"
    ],
    "main": "dist/index.js",
    "bin": {
        "novasheets": "bin/index.js"
    },
    "engines": {
        "node": ">=12"
    },
    "scripts": {
        "build": "tsc && node build",
        "changelog": "git log --format=\"- %h %s\" --reverse -10",
        "test": "tsc && qunit",
        "test:compile": "tsc && node test/test-compile"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NovaSheets/NovaSheets.git"
    },
    "bugs": {
        "url": "https://github.com/NovaSheets/NovaSheets/issues"
    },
    "author": "Nixinova (https://nixinova.com)",
    "homepage": "https://novasheets.js.org",
    "license": "ISC",
    "dependencies": {
        "balanced-match": "^2.0.0",
        "glob": "^7.2.3",
        "js-yaml": "^4.1.0",
        "math-and-unit-parser": "1.3.6"
    },
    "devDependencies": {
        "@types/balanced-match": "^1.0.2",
        "@types/glob": "^8.1.0",
        "@types/node": "ts5.0",
        "brfs": "^2.0.2",
        "browserify": "^17.0.0",
        "qunit": "^2.19.4",
        "typescript": "~5.0.4",
        "uglify-js": "^3.17.4"
    }
}
