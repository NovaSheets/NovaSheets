{
    "name": "novasheets",
    "version": "1.0.3",
    "description": "A simple, powerful, and versatile CSS preprocessor",
    "keywords": [
        "novasheets",
        "nvss",
        "css",
        "css preprocessor",
        "css nesting",
        "nested css",
        "preprocessor",
        "stylesheets",
        "parser"
    ],
    "files": [
        "bin/*.js",
        "src/**/*.js",
        "src/**/*.d.ts",
        "src/**/*.yaml",
        "!src/browser.*"
    ],
    "main": "src/index.js",
    "bin": {
        "novasheets": "bin/index.js"
    },
    "scripts": {
        "prepublish": "tsc",
        "build": "tsc && node build",
        "changelog": "git log --format=\"- %h %s\" --reverse -10",
        "test": "tsc && qunit",
        "test:compile": "node test/test-compile"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NovaSheets/NovaSheets.git"
    },
    "bugs": {
        "url": "https://github.com/NovaSheets/NovaSheets/issues"
    },
    "author": "Nixinova (https://nixinova.com)",
    "homepage": "https://novasheets.js.org",
    "license": "ISC",
    "dependencies": {
        "balanced-match": "^2.0.0",
        "glob": "^7.1.7",
        "js-yaml": "^4.1.0",
        "math-and-unit-parser": "1.3.6"
    },
    "devDependencies": {
        "@types/node": "ts4.3",
        "brfs": "^2.0.2",
        "browserify": "^17.0.0",
        "qunit": "^2.16.0",
        "typescript": "~4.3.5",
        "uglify-js": "^3.13.10"
    }
}
